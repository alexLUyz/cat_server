<%- include("../partials/header") %>

    <div class="container">

        <div class="row text-center" style="display: flex; flex-wrap: wrap;">
            <div class="col-lg-12">
                <h3>Cat Gallery</h3>
            </div>

            <% pics.forEach(function(img) { %>
                <div class="col-md-3 col-sm-6">
                    <div class="thumbnail">
                        <a href="/cats/<%=img.cat%>/posts/<%=img.post%>">
                            <img src="<%=img.img%>">
                        </a>

                    </div>

                </div>
                <% }); %>

        </div>

    </div>


    <%- include("../partials/footer") %>